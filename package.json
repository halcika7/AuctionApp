{
  "name": "backend",
  "version": "1.0.0",
  "description": "Auction App",
  "main": "index.js",
  "scripts": {
    "start": "node index",
    "server": "nodemon index",
    "frontend": "npm start --prefix frontend",
    "frontend-install": "npm install --prefix frontend",
    "create-db": "node ./backend/config/createDb.js",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "dev": "concurrently \"npm run server\" \"npm run frontend\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run migrate && npm run seed && npm install --prefix frontend && npm run build --prefix frontend"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "concurrently": "^5.0.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.3.1",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "pug": "^2.0.4",
    "sequelize": "^5.21.0",
    "sequelize-cli": "^5.5.1",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "faker": "^4.1.0",
    "mocha": "^6.2.1",
    "nodemon": "^1.19.4",
    "should": "^13.2.3",
    "sinon": "^7.5.0",
    "supertest": "^4.0.2"
  }
}
