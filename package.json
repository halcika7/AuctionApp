{
  "name": "backend",
  "version": "1.0.0",
  "description": "Auction App",
  "main": "index.js",
  "scripts": {
    "start": "node index",
    "server": "TZ=utc nodemon index",
    "frontend": "npm start --prefix frontend",
    "frontend-install": "npm install --prefix frontend",
    "create-db": "node ./backend/config/createDb.js",
    "generate-users": "node ./backend/config/createDefaultUsers.js",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "dev": "concurrently \"npm run server\" \"npm run frontend\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend && npm run generate-users && npm run migrate && npm run seed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
